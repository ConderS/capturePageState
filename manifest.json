{
  "name": "Slushman Screenshot",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Takes screenshots at particular sizes.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab" ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
